# configuration file.
cp 000-default.conf /etc/apache2/sites-available
ln -s ../mods-available/cgi.load /etc/apache2/mods-enabled/cgi.load

# Set up directories where stuff goes.
mkdir /home/pi/www
service apache2 restart

ln -s ../images /home/pi/www/pix

